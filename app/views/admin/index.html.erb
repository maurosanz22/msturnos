<div class="row">
  <div class="col-md-6">
    <div class="panel panel-primary">
      <div class="panel-heading">
        Sucursales
      </div>
      <div class="panel-body">
        <table class="table">
          <thead>
            <tr>
              <th>Nombre</th>
              <th colspan="3"></th>
            </tr>
          </thead>
          <tbody>
            <% @branches.each do |branch| %>
            <tr>
                <td><%= branch.nombre %></td>
                <td>
                    <%= link_to admin_path, :class =>"btn btn-warning btn-xs"  do %>
                        <span class="glyphicon glyphicon-list-alt"></span>
                    <% end %>
                </td>
            </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
  <div class="col-md-6">
    <div class="panel panel-info">
      <div class="panel-heading">
        Turnos
      </div>
      <div class="panel-body">
        <table class="table">
          <thead>
            <tr>
              <th>Fecha</th>
              <th>Cliente</th>
              <th colspan="3"></th>
            </tr>
          </thead>
          <tbody>
            <tr>
                <td>10/05/2017</td>
                <td>Lucio</td>
                <td></td>
            </tr>
          </tbody>
        </table>        
      </div>
    </div>    
  </div>
</div>
